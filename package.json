{
  "name": "sequelize-ui",
  "version": "1.0.0",
  "private": true,
  "license": "ISC",
  "scripts": {
    "prepare": "husky install",
    "dev": "next",
    "build": "next build",
    "lint": "eslint --cache --ignore-path .gitignore .",
    "format": "prettier --write src/**/*.{ts,tsx} e2e/**/*.ts",
    "check:format": "prettier --check src/**/*.{ts,tsx} e2e/**/*.ts",
    "check:types": "tsc -b tsconfig.check.json",
    "generate:css-types": "tailwindcss-classnames -i tailwind.config.js -o src/ui/styles/classnames/__generated__/tailwindcss-classnames.ts",
    "generate:jtd-schema:schema": "jtd-codegen src/api/schema/implementations/localStorage/v1/schema.jtd.json --typescript-out src/api/schema/implementations/localStorage/v1 --root-name schema-v1",
    "generate:jtd-schema:user-preferences": "jtd-codegen src/api/userPreferences/implementations/localStorage/v1/schema.jtd.json --typescript-out src/api/userPreferences/implementations/localStorage/v1 --root-name userPreferences-v1",
    "postbuild": "npm run sitemap",
    "sitemap": "next-sitemap",
    "test": "jest",
    "test:coverage": "npm test -- src e2e --coverage",
    "test:unit": "npm test -- src",
    "test:e2e": "jest e2e",
    "test:e2e:migrations": "jest e2e/migrations",
    "test:e2e:migrations:all": "cross-env SQL_DIALECT=all npm run test:e2e:migrations",
    "test:watch": "npm test -- --watch --"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run check:types"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "body-scroll-lock": "^4.0.0-beta.0",
    "change-case": "^4.1.2",
    "clsx": "^2.1.1",
    "copy-to-clipboard": "^3.3.3",
    "dompurify": "^3.1.2",
    "fast-deep-equal": "^3.1.3",
    "file-saver": "^2.0.5",
    "focus-trap": "^7.5.4",
    "jszip": "^3.10.1",
    "jtd": "^0.1.1",
    "marked": "^12.0.2",
    "nanoid": "^5.0.7",
    "next": "^14.2.3",
    "pluralize": "^8.0.0",
    "prism-react-renderer": "^2.3.1",
    "qs": "^6.12.1",
    "react": "~18.3.1",
    "react-dom": "~18.3.1",
    "scroll-into-view-if-needed": "^3.1.0",
    "tailwindcss": "^3.4.3",
    "tailwindcss-children": "^2.1.0",
    "tailwindcss-classnames": "~3.1.0",
    "tar": "^7.0.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.3",
    "@next/eslint-plugin-next": "^14.2.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/body-scroll-lock": "^3.1.2",
    "@types/dompurify": "^3.0.5",
    "@types/file-saver": "^2.0.7",
    "@types/jest-specific-snapshot": "^0.5.9",
    "@types/marked": "^5.0.2",
    "@types/node": "^20.12.7",
    "@types/pg": "^8.11.5",
    "@types/pluralize": "0.0.33",
    "@types/qs": "^6.9.15",
    "@types/react": "~18.3.1",
    "@types/react-dom": "~18.3.0",
    "@types/sqlite3": "^3.1.11",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "autoprefixer": "^10.4.19",
    "check-os": "^1.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-localstorage-mock": "^2.4.26",
    "jest-specific-snapshot": "^8.0.0",
    "lint-staged": "^15.2.2",
    "mysql2": "^3.9.7",
    "next-sitemap": "^4.2.3",
    "pg": "^8.11.5",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "react-test-renderer": "^18.3.1",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.1.7",
    "tedious": "^18.2.0",
    "typescript": "^5.4.5"
  },
  "overrides": {
    "@types/react": "$@types/react",
    "react": "$react",
    "react-dom": "$react-dom",
    "react-test-renderer": "$react-test-renderer"
  }
}
