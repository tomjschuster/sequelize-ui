{
  "name": "sequelize-ui",
  "version": "1.0.0",
  "private": true,
  "license": "ISC",
  "scripts": {
    "prepare": "husky install",
    "dev": "next",
    "build": "next build && next export",
    "build:sitemap": "next-sitemap",
    "start": "next start",
    "lint": "eslint --cache --ignore-path .gitignore .",
    "format": "prettier --write {src,e2e}/**/*.{js,ts,tsx}",
    "check:format": "prettier --check {src,e2e}/**/*.{js,ts,tsx}",
    "check:types": "tsc -b tsconfig.check.json",
    "generate:css-types": "tailwindcss-classnames -i tailwind.config.js -o src/ui/styles/classnames/__generated__/tailwindcss-classnames.ts",
    "generate:jtd-schema:schema": "jtd-codegen src/api/schema/implementations/localStorage/v1/schema.jtd.json --typescript-out src/api/schema/implementations/localStorage/v1 --root-name schema-v1",
    "generate:jtd-schema:user-preferences": "jtd-codegen src/api/userPreferences/implementations/localStorage/v1/schema.jtd.json --typescript-out src/api/userPreferences/implementations/localStorage/v1 --root-name userPreferences-v1",
    "test": "jest",
    "test:coverage": "npm test -- src e2e --coverage",
    "test:unit": "npm test -- src",
    "test:e2e": "jest e2e",
    "test:e2e:migrations": "jest e2e/migrations",
    "test:e2e:migrations:all": "cross-env SQL_DIALECT=all npm run test:e2e:migrations",
    "test:watch": "npm test -- --watch --"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run check:types"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.2",
    "body-scroll-lock": "^4.0.0-beta.0",
    "change-case": "^4.1.2",
    "clsx": "^1.1.1",
    "copy-to-clipboard": "^3.3.1",
    "dompurify": "^2.3.8",
    "fast-deep-equal": "^3.1.3",
    "file-saver": "^2.0.5",
    "focus-trap": "^6.9.3",
    "jszip": "^3.10.0",
    "jtd": "^0.1.1",
    "marked": "^4.0.16",
    "nanoid": "^3.3.4",
    "next": "^12.1.6",
    "pluralize": "^8.0.0",
    "prism-react-renderer": "^1.3.3",
    "qs": "^6.10.3",
    "react": "~18.1.0",
    "react-dom": "~18.1.0",
    "scroll-into-view-if-needed": "^2.2.29",
    "tailwindcss": "^3.0.24",
    "tailwindcss-children": "^2.1.0",
    "tailwindcss-classnames": "~3.0.5"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.1.6",
    "@next/eslint-plugin-next": "^12.1.6",
    "@testing-library/react-hooks": "^8.0.0",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/dompurify": "^2.3.3",
    "@types/file-saver": "^2.0.5",
    "@types/jest-specific-snapshot": "^0.5.5",
    "@types/marked": "^4.0.3",
    "@types/node": "^17.0.36",
    "@types/pg": "^8.6.5",
    "@types/pluralize": "0.0.29",
    "@types/qs": "^6.9.7",
    "@types/react": "~18.0.9",
    "@types/react-dom": "~18.0.5",
    "@types/sqlite3": "^3.1.8",
    "@types/tedious": "^4.0.7",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "autoprefixer": "^10.4.7",
    "check-os": "^1.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.16.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-localstorage-mock": "^2.4.21",
    "jest-specific-snapshot": "^5.0.0",
    "lint-staged": "^12.4.2",
    "mysql2": "^2.3.3",
    "next-sitemap": "^2.5.27",
    "pg": "^8.7.3",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "react-test-renderer": "^18.1.0",
    "source-map-support": "^0.5.21",
    "sqlite3": "npm:@vscode/sqlite3@^5.0.8",
    "tedious": "^14.5.0",
    "typescript": "^4.7.2"
  }
}
